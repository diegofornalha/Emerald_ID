pub fun main(publicKey: String): Bool {
    let bytes = publicKey.decodeHex()
    let key = PublicKey(
      publicKey: bytes,
      signatureAlgorithm: SignatureAlgorithm.ECDSA_P256
    )
    let sig = "bbc860b790b2a1be3d1cb01d2f9a694651d5514d303bd762c3849357d09f34aeb9cd2f186255968050351e9948baa674705a9275c95d1d030a6ceb895b334b90".decodeHex()
    log(sig)
    let data = "".decodeHex()
    log(data)
    return key.verify(signature: sig, signedData: data, domainSeparationTag: "FLOW-V0.0-user", hashAlgorithm: HashAlgorithm.SHA3_256)
}